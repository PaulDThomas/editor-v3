{
  "name": "@asup/editor-v3",
  "version": "1.2.0",
  "description": "REACT Typescript Editor component",
  "author": "Paul Thomas <@PaulDThomas>",
  "private": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulDThomas/editor-v3.git"
  },
  "keywords": [
    "react",
    "typescript",
    "editor"
  ],
  "bugs": {
    "url": "https://github.com/PaulDThomas/editor-v3/issues"
  },
  "homepage": "https://github.com/PaulDThomas/editor-v3#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "dist/cjs/main.js",
  "module": "dist/main.js",
  "types": "dist/editor-v3.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "parcel build src/main.ts",
    "start": "rmdir /q/s .\\.parcel-cache && parcel demo/index.html --dist-dir demo/dist",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@asup/context-menu": "^1.0.1",
    "@swc/helpers": "^0.5.1",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "staticFiles": [
    {
      "staticPath": "demo/public/static",
      "staticOutPath": "public",
      "env": {
        "NODE_ENV": "development"
      }
    }
  ],
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.7.0",
    "@parcel/packager-ts": "^2.8.3",
    "@parcel/transformer-sass": "^2.7.0",
    "@parcel/transformer-typescript-types": "^2.8.0",
    "@parcel/transformer-webmanifest": "^2.7.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.2.2",
    "lint-staged": "^15.1.0",
    "parcel": "^2.7.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "postcss": "^8.4.18",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "@parcel/transformer-js": {
    "inlineEnvironment": [
      "REACT_APP_*",
      "npm_package_*"
    ]
  },
  "@parcel/transformer-css": {
    "cssModules": {
      "pattern": "mos-[name]-[hash]-[local]",
      "dashedIndents": true
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
